{{> head}}
{{> header}}

    <main>
        <h1>This is a header</h1>
        <section>
            This is the body
            <div id='movieList'></div>
        </section>
    </main>

{{> footer}}

<script>
    const url = "https://api.themoviedb.org/3/movie/now_playing?api_key=a611315ba9b178c672528a504436308c"

    fetch(url)
    .then(
        function(response) {
            console.log(response)
            return response.json();
        }
    )
    .then(
        function(json){
            console.log(json);
            document.getElementById('movieList').innerHTML = json.results
        }
    )
</script>